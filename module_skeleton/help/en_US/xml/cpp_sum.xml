	<language>en_US</language>
	<keyword>cpp_sum</keyword>
	<short_description>Return sum (cpp version).</short_description>

	<syntax>
		<syntax_item>nelson_sum(A, B)</syntax_item>
		<syntax_item>C = nelson_sum(A, B)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>A</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>B</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>C</param_name>
			<param_description>result of A + B</param_description>
		</param_output_item>
	</param_output>

	<description><p><b>C = nelson_sum(A, B)</b> returns the sum of A and B variables.</p>
</description>


	<used_function>addition</used_function>
